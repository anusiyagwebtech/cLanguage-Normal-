#include <stdio.h>


void cubeFinder(int num);

int main () {
	
//   Write a program in C to display the cube of the number up to an integer.
    
   cubeFinder(5);
    
	
	return 0;
}

void cubeFinder (int num) {
	for (int i = 1; i <= num ; i++) {
	  printf("Number is : %d and cube of the %d is :%d\n",i,i,i*i*i );	
	}

}

