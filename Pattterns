for (int j = 5; j > 0; j--) {
      	for (int i = 0; i < j; i++) {
    	printf("%d ",j);
    
	} printf("\n");
	    for (int k = j-1; k < 5;  k++) {
	    	printf(" ");
		}
	  } 

